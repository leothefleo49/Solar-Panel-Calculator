{
  "name": "solar-panel-calculator",
  "private": false,
  "version": "1.4.11",
  "description": "Professional solar PV financial & technical analysis dashboard with Google Solar API integration, multi-AI chat assistant, and cross-platform support (Desktop, Mobile, Web)",
  "author": "leothefleo49",
  "license": "MIT",
  "keywords": [
    "solar",
    "solar-panel",
    "pv-calculator",
    "solar-energy",
    "roi-calculator",
    "financial-analysis",
    "google-solar-api",
    "ai-assistant",
    "tauri",
    "capacitor",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leothefleo49/Solar-Panel-Calculator.git"
  },
  "bugs": {
    "url": "https://github.com/leothefleo49/Solar-Panel-Calculator/issues"
  },
  "homepage": "https://github.com/leothefleo49/Solar-Panel-Calculator#readme",
  "type": "module",
  "scripts": {
    "preinstall": "node -e \"console.log('Checking system dependencies...')\"",
    "postinstall": "node scripts/check-deps.cjs",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "npm run check:rust && tauri dev",
    "tauri:build": "npm run check:rust && tauri build",
    "check:rust": "node scripts/check-deps.cjs rust",
    "cap:sync": "npm run build && npx cap sync",
    "cap:android": "npm run build && npx cap sync android && npx cap open android",
    "cap:build:android": "npm run build && npx cap sync android && cd android && ./gradlew assembleRelease",
    "cap:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "cap:sync:ios": "npm run build && npx cap sync ios",
    "smoke:test": "node scripts/smoke-test.mjs",
    "generate:update-manifest": "node scripts/generate-update-manifest.mjs"
  },
  "dependencies": {
    "@capacitor/browser": "^7.0.2",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "@tauri-apps/plugin-store": "^2.4.1",
    "@tauri-apps/plugin-updater": "^2",
    "clsx": "^2.1.1",
    "decimal.js": "^10.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-is": "^19.2.0",
    "recharts": "^3.4.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^2.9.4",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}
