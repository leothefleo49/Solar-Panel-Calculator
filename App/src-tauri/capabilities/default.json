{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-set-fullscreen",
    "core:window:allow-is-fullscreen",
    "shell:allow-open",
    "dialog:default",
    "fs:default",
    "updater:default",
    "http:default",
    "process:default",
    "process:allow-restart"
  ]
}
